{
  "name": "daemons.storage.backend",
  "version": "1.0.0",
  "description": "Scripts storage for Daemons",
  "main": "src/index.ts",
  "scripts": {
    "test": "mocha ./**/*test.ts",
    "test:ci": "mocha ./**/*test.ts --exit",
    "start": "node dist/index.js",
    "build": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only --exit-child --watch src src/index.ts | pino-pretty -c -i time,hostname,source -S -o '[{source}] {msg}'"
  },
  "author": "Daemons Finance",
  "license": "ISC",
  "dependencies": {
    "@daemons-fi/db-schema": "0.0.6",
    "body-parser": "^1.19.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "ethers": "^5.7.0",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.5.3",
    "pino": "^8.4.2",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@types/chai": "^4.3.3",
    "@types/chai-as-promised": "^7.1.5",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/mocha": "^9.1.1",
    "@types/node": "18.7.13",
    "@types/sinon": "^10.0.13",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "mocha": "10.0.0",
    "mocha-param": "^2.0.1",
    "mongodb-memory-server": "^8.9.0",
    "pino-pretty": "9.1.0",
    "prettier": "^2.7.1",
    "sinon": "14.0.0",
    "supertest": "^6.2.4",
    "ts-node-dev": "2.0.0",
    "typescript": "^4.8.2"
  },
  "mocha": {
    "spec": "./**/*test.ts",
    "ignore": "./node_modules/**",
    "require": "ts-node/register/files",
    "timeout": 20000
  }
}
