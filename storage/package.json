{
  "name": "daemons.storage",
  "version": "1.0.0",
  "description": "Scripts storage for Daemons",
  "main": "src/index.ts",
  "scripts": {
    "test": "mocha ./**/*test.ts",
    "test:ci": "mocha ./**/*test.ts --exit",
    "start": "node dist/index.js",
    "build": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only --exit-child --watch src src/index.ts | pino-pretty -c -i time,hostname,source -S -o '[{source}] {msg}'"
  },
  "author": "Daemons Finance",
  "license": "ISC",
  "dependencies": {
    "@daemons-fi/db-schema": "0.0.2",
    "body-parser": "^1.19.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "ethers": "^5.5.3",
    "express": "^4.17.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.1.6",
    "pino": "^8.3.0",
    "ts-node": "^10.7.0"
  },
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/chai-as-promised": "^7.1.5",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/mocha": "^9.0.0",
    "@types/node": "^17.0.31",
    "@types/sinon": "^10.0.11",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "mocha": "^9.1.4",
    "mocha-param": "^2.0.1",
    "mongodb-memory-server": "^8.2.0",
    "pino-pretty": "^8.1.0",
    "prettier": "^2.5.1",
    "sinon": "^13.0.1",
    "supertest": "^6.2.1",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.4"
  },
  "mocha": {
    "spec": "./**/*test.ts",
    "ignore": "./node_modules/**",
    "require": "ts-node/register/files",
    "timeout": 20000
  }
}
