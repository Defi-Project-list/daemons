{
    "name": "daemons.admin.backend",
    "version": "1.0.0",
    "description": "Runs tasks for Daemons",
    "main": "src/index.ts",
    "scripts": {
        "start": "node dist/index.js",
        "build": "tsc",
        "dev": "ts-node-dev --respawn --transpile-only --exit-child --watch src . | pino-pretty -c -i time,hostname,source -S -o '[{source}] {msg}'"
    },
    "author": "Daemons Finance",
    "license": "ISC",
    "dependencies": {
        "@daemons-fi/contracts": "0.0.11",
        "cron": "2.1.0",
        "cross-fetch": "^3.1.5",
        "dotenv": "^16.0.1",
        "ethers": "^5.7.0",
        "express": "^4.18.1",
        "mongoose": "^6.5.3",
        "pino": "^8.4.2"
    },
    "devDependencies": {
        "@types/chai": "^4.3.3",
        "@types/cron": "2.0.0",
        "@types/express": "^4.17.13",
        "@types/mocha": "^9.1.1",
        "@types/node": "18.7.13",
        "@types/node-cron": "^3.0.2",
        "chai": "^4.3.6",
        "mocha": "10.0.0",
        "pino-pretty": "9.1.0",
        "ts-node-dev": "2.0.0",
        "typescript": "^4.8.2"
    },
    "mocha": {
        "spec": "./**/*test.ts",
        "ignore": "./node_modules/**",
        "timeout": 20000
    }
}
